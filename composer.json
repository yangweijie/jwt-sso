{
    "name": "yangweijie/jwt-sso",
    "description": "sso with firebase/Jwt",
    "keywords": [
        "yangweijie/jwt-sso",
        "jwt-sso"
    ],
    "homepage": "https://github.com/yangweijie/jwt-sso",
    "license": "MIT",
    "authors": [
        {
            "name": "yangweijie",
            "email": "917647288@qq.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "firebase/php-jwt" : "6.*",
        "nesbot/carbon": "^1.0 || ^2.0 || ^3.0",
        "juststeveking/php-sdk": "^1.0 || ^2.0 || ^3.0",
        "laravel/framework": "^11.11.1"
    },
    "require-dev": {
        "pestphp/pest": "^2.15",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "yangweijie\\jwt\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "yangweijie\\jwt\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "extra": {
        "laravel": {
            "providers": [
                "yangweijie\\jwt\\JwtSsoServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
